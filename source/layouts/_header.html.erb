<header id="Header">
    <div class="title">
        <svg viewBox="0 0 900 240" aria-label="Ibaiak Logo">
            <use xlink:href="./assets/svg/svg.svg#logo-ibaiak"></use>
        </svg>
    </div>

    <button class="button-menu __btnFluid" data-toggle-window="win-menu">
        <span class="icons">
            <svg class="waves" viewBox="0 0 30 30" aria-hidden="true">
                <!-- <use xlink:href="./assets/svg/svg.svg#icon-waves-anim"></use> -->
                <path vector-effect="non-scaling-stroke" d="M60,13.8c-5,0-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3"/>
                <path vector-effect="non-scaling-stroke" d="M60,21c-5,0-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3"/>
                <path vector-effect="non-scaling-stroke" d="M60,6.6c-5,0-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3s-5,3-10,3s-5-3-10-3"/>
            </svg>

            <svg class="cross" viewBox="0 0 32 32" aria-hidden="true">
                <use xlink:href="./assets/svg/svg.svg#icon-close"></use>
            </svg>
        </span>

        <span class="holder-text">
            <span class="holder">
                <span class="text">Rio Nervión</span>
                <span class="text">Cerrar</span>
            </span>
        </span>

    </button>

    <div class="header__years" aria-label="Selector de años">
        <span aria-hidden="true">Fotografías desde</span>
        <div class="select-with-arrow">
            <label for="year-start" class="sr-only">Elige año de inicio:</label>
            <select id="year-start" name="year-start">
                <option>2021</option>
                <option>2020</option>
                <option>2019</option>
                <option>2018</option>
                <option>2017</option>
                <option>2016</option>
                <option>2015</option>
                <option>2014</option>
                <option>2013</option>
            </select>
        </div>
        <span aria-hidden="true">hasta</span>

        <div class="select-with-arrow">
            <label for="year-end" class="sr-only">Elige año de fin:</label>
            <select id="year-end" name="year-end">
                <option>2021</option>
                <option>2020</option>
                <option>2019</option>
                <option>2018</option>
                <option>2017</option>
                <option>2016</option>
                <option>2015</option>
                <option>2014</option>
                <option>2013</option>
            </select>
        </div>
    </div>
</header>